{
  "name": "avtojon",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start": "concurrently \"npm run api:dev\" \"npm run web:dev\"",
    "dev": "concurrently \"npm run api:dev\" \"npm run web:dev\"",
    "api:dev": "npm run dev --workspace=apps/api",
    "web:dev": "npm run dev --workspace=apps/web",
    "api:start": "npm run start --workspace=apps/api",
    "install:all": "npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.1.0"
  }
}
