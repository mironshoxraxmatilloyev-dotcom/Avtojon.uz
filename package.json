{
  "name": "avtojon",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/api",
    "apps/web",
    "packages/*"
  ],
  "scripts": {
    "start": "concurrently \"npm run api:dev\" \"npm run web:dev\"",
    "dev": "concurrently \"npm run api:dev\" \"npm run web:dev\"",
    "build": "npm run build --workspace=apps/web",
    "build:api": "npm run build --workspace=apps/api",
    "build:web": "npm run build --workspace=apps/web",
    "api:dev": "npm run dev --workspace=apps/api",
    "web:dev": "npm run dev --workspace=apps/web",
    "api:start": "npm run start --workspace=apps/api",
    "install:all": "npm install"
  },
  "devDependencies": {
    "@react-native/gradle-plugin": "^0.74.87",
    "concurrently": "^8.2.2",
    "prettier": "^3.1.0"
  },
  "overrides": {
    "react": "18.2.0"
  }
}
