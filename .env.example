# Server
NODE_ENV=development
PORT=3000

# MongoDB
# MongoDB Atlas connection string
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/avtojon?retryWrites=true&w=majority

# JWT
# Kuchli va uzun secret key ishlatish tavsiya etiladi
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# CORS
# Vergul bilan ajratilgan URL lar
CORS_ORIGINS=http://localhost:5173,http://127.0.0.1:5173
